{
  "name": "placeholder-site",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/placeholder-site/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/vite:build",
      "outputs": [
        "{options.outputPath}"
      ],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/apps/placeholder-site"
      },
      "configurations": {
        "development": {
          "mode": "development"
        },
        "production": {
          "mode": "production",
          "fileReplacements": [
            {
              "replace": "apps/placeholder-site/src/environments/environment.ts",
              "with": "apps/placeholder-site/src/environments/environment.production.ts"
            }
          ]
        }
      }
    },
    "serve": {
      "executor": "@nx/vite:dev-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "placeholder-site:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "placeholder-site:build:development",
          "hmr": true,
          "port": 4201
        },
        "production": {
          "buildTarget": "placeholder-site:build:production",
          "hmr": false
        }
      }
    },
    "preview": {
      "executor": "@nx/vite:preview-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "placeholder-site:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "placeholder-site:build:development"
        },
        "production": {
          "buildTarget": "placeholder-site:build:production"
        }
      }
    },
    "test": {
      "executor": "@nx/vite:test",
      "outputs": [
        "{workspaceRoot}/coverage/apps/placeholder-site"
      ],
      "options": {
        "passWithNoTests": true,
        "reportsDirectory": "../../coverage/apps/placeholder-site"
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "apps/placeholder-site/**/*.{ts,tsx,js,jsx}"
        ]
      }
    }
  },
  "tags": []
}
